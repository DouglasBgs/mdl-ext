{
  "name": "mdlext-example",
  "version": "0.0.1",
  "description": "mdl-ext custom build",
  "author": "Leif Olsen <leif.o.olsen.at.gmail.com> (https://github.com/leifoolsen)",
  "main": "src/index.js",
  "scripts": {
    "setup": "npm install && npm-run-all clean validate",
    "clean": "rimraf dist",
    "validate": "npm run lint",
    "prebuild": "npm-run-all clean validate",
    "build": "webpack --env.dev --progress",
    "prebuild:prod": "npm-run-all clean validate",
    "build:prod": "NODE_ENV=production webpack --env.prod --progress",
    "lint": "npm-run-all lint:*",
    "lint:stylelint": "stylelint './src/**/*.s?(a|c)ss' --syntax scss",
    "lint:eslint": "eslint ./src",
    "analyze": "webpack --env.prod --json | webpack-bundle-size-analyzer",
    "server": "babel-node server --env.prod --port 8081",
    "start": "babel-node server --env.dev --hot --port 8080"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6"
  },
  "dependencies": {
    "babel-polyfill": "6.22.0",
    "material-design-lite": "1.3.0",
    "mdl-ext": "0.14.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.0",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-preset-latest": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "body-parser": "1.16.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "eslint": "3.14.0",
    "eslint-loader": "1.6.1",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.26.0",
    "ignore-styles": "5.0.1",
    "imports-loader": "0.7.0",
    "ip": "1.1.4",
    "jsdom": "9.9.1",
    "json-loader": "0.5.4",
    "material-design-icons": "3.0.1",
    "node-sass": "4.3.0",
    "npm-run-all": "4.0.1",
    "postcss-loader": "1.2.2",
    "precss": "1.4.0",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "1.6.1",
    "rimraf": "2.5.4",
    "roboto-fontface": "0.7.0",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "stylelint": "7.7.1",
    "stylelint-config-standard": "15.0.1",
    "stylelint-webpack-plugin": "0.5.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.0",
    "webpack-bundle-size-analyzer": "2.5.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0"
  },
  "keywords": [
    "mdl-ext"
  ],
  "license": "MIT"
}
